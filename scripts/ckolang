#!/bin/bash

export PATH=$PATH:~/defects4j/framework/bin
export http_proxy="http://proxy:3128/"
export https_proxy="https://proxy:3128/"

echo "Starting checkout Lang ..."
for bug in `seq 1 65`;
do
	echo 'Checkout Lang_'$bug'_buggy'	
	defects4j checkout -p Lang -v $bug'b' -w ~/ressources/Lang_$bug'_buggy'
done
