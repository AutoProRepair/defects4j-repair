#!/bin/bash

export PATH="~/defects4j/framework/bin:$PATH"

cp -r ~/projects/math/math_${1}/ /tmp/
cd /tmp/math_${1}/
defects4j compile

cp ~/nopol/nopol-0.0.3-SNAPSHOT-jar-with-dependencies.jar .
cp -r ~/nopol/z3-x64-debian-7.7/ .
mkdir lib/
cp ~/defects4j/framework/projects/Math/lib/commons-discovery-0.5.jar lib/

if [[ $1 -lt 85 ]]; then
  time java -cp nopol-0.0.3-SNAPSHOT-jar-with-dependencies.jar fr.inria.lille.repair.Main repair precondition symbolic src/main/java/ target/classes/:target/test-classes/:lib/commons-discovery-0.5.jar:nopol-0.0.3-SNAPSHOT-jar-with-dependencies.jar z3 z3-x64-debian-7.7/bin/z3
else
  time java -cp nopol-0.0.3-SNAPSHOT-jar-with-dependencies.jar fr.inria.lille.repair.Main repair precondition symbolic src/java/ target/classes/:target/test-classes/:lib/commons-discovery-0.5.jar:nopol-0.0.3-SNAPSHOT-jar-with-dependencies.jar z3 z3-x64-debian-7.7/bin/z3
fi

cd /tmp/
rm -rf math_${1}/
